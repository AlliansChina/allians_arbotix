  </gazebo>
<!---==================================================================================================================================-->
  <link name="imu_link">
    <inertial>
      <mass value="0.001"/>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.000001" iyz="0" izz="0.0001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <geometry>
        <box size=".001 .001 .001"/>
      </geometry>
    </collision>
  </link>
  <joint
    name="imu"
    type="fixed">
    <origin
      xyz="0 0 0.15"
      rpy="0 0 0" />
    <parent

      link="base_link" />
    <child

      link="imu_link" />
    <axis
      xyz="0 0 0" />
  </joint>
<!---==================================================================================================================================-->
  <gazebo>
     <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
        <alwaysOn>true</alwaysOn>
        <updateRate>50</updateRate>
        <leftJoint>right_wheel</leftJoint>
        <rightJoint>left_wheel</rightJoint>
        <wheelSeparation>0.597</wheelSeparation>
        <wheelDiameter>0.3</wheelDiameter>
        <wheelTorque>40</wheelTorque>
        <commandTopic>cmd_vel</commandTopic>
        <odometryTopic>odom</odometryTopic>
        <odometryFrame>odom</odometryFrame>
        <robotBaseFrame>base_link</robotBaseFrame>
     </plugin>
  </gazebo>

<!---==================================================================================================================================-->
  <gazebo>
    <plugin filename="libgazebo_ros_imu.so" name="ros_imu">
      <updateRate>100</updateRate>
      <robotNamespace>/mover</robotNamespace>
      <topicName>imu_data</topicName>
      <bodyName>imu_link</bodyName>
      <frameName>base_link</frameName>
      <serviceName>/imu_service</serviceName>
      <gaussianNoise>0.005</gaussianNoise>
      <imu>
        <noise>
          <type>gaussian</type>
          <rate>
            <mean>0.0</mean>
            <stddev>2e-4</stddev>
            <bias_mean>0.0000075</bias_mean>
            <bias_stddev>0.0000008</bias_stddev>
          </rate>
          <accel>
            <mean>0.0</mean>
            <stddev>1.7e-2</stddev>
            <bias_mean>0.1</bias_mean>
            <bias_stddev>0.001</bias_stddev>
          </accel>
        </noise>
      </imu>
    </plugin>
  </gazebo>
</robot>
